<map proto="1.4.1">
<name>Chimeric II</name>
<version>1.0.0</version>
<objective>Capture the other team's wool and return it to your base, and destroy their monument.</objective>
<authors>
    <author uuid="f876b863-1ccd-4ae2-acad-8a09e2209e81"/>
    <!--  _DISTRACTION_  -->
    <author uuid="af56fc4a-51d2-4a6e-b020-2c6d272b9cbf"/>
    <!--  Ale  -->
</authors>
<broadcasts>
    <alert after="1s" every="2m">You must capture your team's wool before mining the monument!</alert>
    <alert after="7s" every="2m">The monument can only be mined with pickaxes from the wool room!</alert> 
    <tip after="14s" every="2m">The wool room is located on the top of the map, behind each teams' spawns!</tip>
    <tip after="21s" every="2m">The monument is located on the center lane of each teams' sides!</tip>
</broadcasts>
<teams>
    <team id="green" color="dark green" max="18">Green</team>
    <team id="purple" color="dark purple" max="18">Purple</team>
</teams>
<maxbuildheight>66</maxbuildheight>
<spawns>
    <spawn team="green" kit="green-kit" yaw="-90">
        <region>
            <cylinder base="-81,27,0" radius="1" height="0"/>
        </region>
    </spawn>
    <spawn team="purple" kit="purple-kit" yaw="90">
        <region>
            <cylinder base="81,27,0" radius="1" height="0"/>
        </region>
    </spawn>
    <default yaw="180">
        <region>
            <point>-0,85,1</point>
        </region>
    </default>
</spawns>
<crafting>
    <disable>workbench</disable>
    <disable>stick</disable>
</crafting>
